{
  "name": "cozy-konnector-libs",
  "version": "4.14.3",
  "description": "All the libs needed by a cozy v3 konnector",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cozy/cozy-konnector-libs.git"
  },
  "keywords": [
    "cozy-v3",
    "konnector"
  ],
  "author": "doubleface",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cozy/cozy-konnector-libs/issues"
  },
  "homepage": "https://github.com/cozy/cozy-konnector-libs#readme",
  "dependencies": {
    "bluebird": "3.5.3",
    "bluebird-retry": "0.11.0",
    "btoa": "1.2.1",
    "cheerio": "1.0.0-rc.2",
    "cozy-client-js": "0.9.0",
    "cozy-logger": "1.3.1",
    "date-fns": "1.30.1",
    "geco": "0.11.1",
    "isomorphic-fetch": "2.2.1",
    "lodash": "4.17.11",
    "lodash-id": "^0.14.0",
    "lowdb": "1.0.0",
    "pdfjs": "2.1.0",
    "pdfjs-dist": "2.2.2",
    "raven": "2.6.4",
    "raw-body": "2.3.3",
    "request": "2.88.0",
    "request-debug": "0.2.0",
    "request-promise": "4.2.2"
  },
  "scripts": {
    "build": "npm run transpile",
    "transpile": "rm -r dist/* ; babel src --out-dir dist",
    "prepublishOnly": "yarn run transpile",
    "test": "cross-env LOG_LEVEL=info jest ./src",
    "docs": "jsdoc2md --template jsdoc2md/README.hbs src/libs/*.js src/helpers/*.js > docs/api.md"
  },
  "standard": {
    "globals": [
      "jest",
      "describe",
      "test",
      "beforeEach",
      "expect",
      "it"
    ]
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-preset-env": "1.7.0",
    "body-parser": "1.18.3",
    "core-js": "2.5.7",
    "cross-env": "5.2.0",
    "eslint-config-cozy-app": "1.1.8",
    "eslint-plugin-prettier": "3.0.1",
    "express": "4.16.4",
    "jest": "22.4.4",
    "jsdoc-to-markdown": "4.0.1",
    "lerna": "3.10.8"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  }
}
